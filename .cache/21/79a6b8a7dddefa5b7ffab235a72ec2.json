ntListener(\"dataavailable\", function (e) {\n    if (e.data.size > 0) {\n      console.log(e.data);\n      recordedChunks.push(e.data);\n    }\n  });\n\n  mediaRecorder.addEventListener(\"stop\", function () {\n    let blob = new Blob(recordedChunks);\n    let audio = document.querySelector(\"audio\");\n    audio.src = window.URL.createObjectURL(blob);\n\n    // if video data\n    // let blob = new Blob(chunks, { 'type': 'video/mp4' });\n    // let video = document.querySelector('video');\n    // video.src = window.URL.createObjectURL(blob);\n  });\n}\n\nrecordStartButton.addEventListener(\"click\", () => mediaRecorder.start());\n\nrecordStopButton.addEventListener(\"click\", () => mediaRecorder.stop());\n"},"lineCount":null}},"error":null,"hash":"c49b7247a9ada4b52d1304e36cd0ac9e","cacheData":{"env":{}}}